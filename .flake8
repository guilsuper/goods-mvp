[flake8]
max-line-length=79
exclude=
        .git,
        __pycache__,
        .tox,
        .eggs,
        *.egg,
        venv,
        pip-download-cache,
        migrations,
        *.md,
        .flake8,
        .pre-commit-config.yaml

ignore=
        W503,
        F811

docstring-convention=google
inline-quotes="
import-order-style=cryptography
